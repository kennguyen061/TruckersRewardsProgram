{
  "name": "coreui",
  "version": "0.4.2-alpha2",
  "description": "Platform Core UI",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab.imshealth.com/platform/coreui.git"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-preset-jest": "^15.0.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "6.11.1",
    "cross-env": "^2.0.1",
    "css-loader": "^0.24.0",
    "devboard": "^0.7.1",
    "eslint": "^3.4.0",
    "eslint-config-coreui": "^0.1.2",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "file-loader": "^0.9.0",
    "jest": "^15.1.1",
    "jest-cli": "^15.1.1",
    "kotatsu": "^0.14.0",
    "moment": "^2.14.1",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-test-renderer": "^15.3.1",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.4"
  },
  "peerDependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "kotatsu": {
    "config": "--public cards/public/ --index cards/public/index.html --devtool \\#cheap-source-map --babel --config cards/webpack.config.js --port 3003 cards/main.js"
  },
  "scripts": {
    "clean": "rimraf ./node_modules/*",
    "clean:lib": "rimraf ./lib/*",
    "clean:dist": "rimraf ./dist/*",
    "compile:lib": "babel src --out-dir lib",
    "compile:dist": "webpack --config config/browser.config.js",
    "build:lib": "npm run clean:lib && npm run compile:lib",
    "build:dist": "npm run clean:dist && npm run compile:dist",
    "build": "cross-env NODE_PATH=src npm run build:lib && npm run build:dist",
    "eslint": "eslint src",
    "prestatic": "rimraf cards/public/build",
    "start": "node scripts/start.js",
    "static": "cross-env NODE_ENV=production node scripts/static.js",
    "test": "jest"
  },
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "coreui-deps": "0.1.0-alpha13",
    "jss": "^5.5.5",
    "jss-preset-default": "^0.5.0",
    "property-expr": "^1.3.1",
    "ramda": "^0.22.1",
    "react-jss": "^4.1.1",
    "recompose": "^0.20.2",
    "yup": "^0.21.1"
  },
  "eslintConfig": {
    "extends": "./config/eslint.js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./build/static/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/(build|cards|config|dist|lib|scripts)"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 25,
        "functions": 60,
        "lines": 80
      }
    },
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(jpg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/config/jest/FileStub.js",
      "^[./a-zA-Z0-9$_-]+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "scriptPreprocessor": "<rootDir>/config/jest/transform.js",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/(build|cards|config|dist|lib|scripts|node_modules)/"
    ],
    "testRegex": "(__tests__/.*|\\.(test|spec))\\.js$",
    "verbose": true
  }
}
